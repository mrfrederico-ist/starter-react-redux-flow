{
  "extends": [
    "eslint:recommended",
    "react-app",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": false,
        "singleQuote": true,
        "jsxBracketSameLine": true,
        "parser": "flow"
      }
    ]
  }
}